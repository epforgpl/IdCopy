{
    "name": "mp/id-copy",
    "description": "Laravel Nova field component that adds a “copy to clipboard” button for ID fields.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mp\\IdCopy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mp\\IdCopy\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/epforgpl/IdCopy.git"
        }
    ]
}
